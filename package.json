{
  "name": "encorelando",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "fi": "^1.0.16",
    "leaflet": "^1.9.4",
    "luxon": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=src/.*\\.test\\.js$ --watchAll=false",
    "test:services": "react-scripts test --testPathPattern=src/services/.*\\.test\\.js$ --watchAll=false",
    "test:components": "react-scripts test --testPathPattern=src/components/.*\\.test\\.js$ --watchAll=false",
    "test:hooks": "react-scripts test --testPathPattern=src/hooks/.*\\.test\\.js$ --watchAll=false",
    "test:watch": "react-scripts test --watch",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "netlify:dev": "netlify dev",
    "download-maps": "node ./scripts/download-static-maps.js",
    "prebuild": "npm run download-maps",
    "scraper:test": "netlify functions:invoke --name scraper --no-identity --payload '{\"runId\": \"test\", \"type\": \"all\", \"forceUpdate\": true}'",
    "create-run": "netlify functions:invoke --name create-scraping-run --no-identity",
    "test-connection": "netlify functions:invoke --name test-connection --no-identity",
    "dev:functions": "netlify dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest-axe": "^7.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^1.2.1",
    "netlify-cli": "^14.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/test/**/*",
      "!src/**/__mocks__/**/*",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 70,
        "functions": 80,
        "lines": 80
      },
      "./src/services/": {
        "statements": 90,
        "branches": 80,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
